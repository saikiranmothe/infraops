
<%# Displays list of @aws_instance_types %>
<div id="div_aws_instance_type_collections" class="mt-10">
	<% @aws_instance_types.each do |aws_instance_type| %>
		<div id="div_aws_instance_type_<%= aws_instance_type.id %>">
			<%= render :partial=>"item", :locals=>{:aws_instance_type=>aws_instance_type} %>
		</div>
	<% end %>
	<% if @aws_instance_types.blank? %>
		<div id="div_aws_instance_type_empty" class="box text-color-grey p-80 text-align-center" style="height:200px;">
      <%= translate("forms.no_results_found") %>
		</div>
	<% end %>
</div>
<div class="cl"></div>

<% if @aws_instance_types.any? %>
   <%= content_tag :div, :class=>"pull-right" do %>
     <%= paginate @aws_instance_types, :remote => true %>
   <% end %>
<% end %>
<div class="cl"></div>
