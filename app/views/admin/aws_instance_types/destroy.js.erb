<% if @aws_instance_type %>
  <% if @aws_instance_type.new_record? %>

  	// Fill the right box with first aws_instance_type details
  	$('#div_aws_instance_type_details').html("<%= escape_javascript(render(:partial=>'summary')) %>");

  <% else %>

  	// Render the show partial to show flash message
  	$('#div_aws_instance_type_details').html("<%= escape_javascript(render(:partial=>'show')) %>");

  <% end %>

  // Render aws_instance_types List
  $('#div_aws_instance_type_collections').html("<%= escape_javascript(render(:partial=>'index')) %>");

<% else %>

  var noResultsText = "<div id=\"div_admin_empty\" class=\"box text-color-grey p-80 text-align-center\" style=\"height:200px;\"><%= translate("forms.no_results_found") %></div>";
  $('#div_aws_instance_type_index').html(noResultsText);
  $('#div_aws_instance_type_details').html(noResultsText);

<% end %>
